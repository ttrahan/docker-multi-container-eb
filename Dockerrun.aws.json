{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "node-app",
      "image": "288971733297.dkr.ecr.us-east-1.amazonaws.com/eb-docker-multi-container-node:master.10",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8000,
          "containerPort": 8000
        }
      ],
      "mountPoints": []
    },
    {
      "name": "tomcat-app",
      "image": "288971733297.dkr.ecr.us-east-1.amazonaws.com/eb-docker-multi-container-tomcat:master.10",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "links": [
        "node-app"
      ],
      "mountPoints": []
    }
  ]
}
